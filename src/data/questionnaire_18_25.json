{
    "ageRange": "18-25",
    "title": "Bilan de Prévention 18-25 ans",
    "sections": [
        {
            "id": "situation",
            "title": "Situation Personnelle, Environnement Social et Familial",
            "questions": [
                {
                    "id": "q1",
                    "text": "Vous êtes :",
                    "type": "multiple",
                    "options": [
                        "Une femme",
                        "Un homme",
                        "Autre"
                    ],
                    "required": true
                },
                {
                    "id": "q2",
                    "text": "Vous êtes :",
                    "type": "multiple",
                    "options": [
                        "En études",
                        "En emploi",
                        "À la recherche d'un emploi",
                        "En cumul étude-emploi",
                        "Autre"
                    ],
                    "required": true
                },
                {
                    "id": "q3",
                    "text": "Vous habitez :",
                    "type": "multiple",
                    "options": [
                        "Seul(e)",
                        "Chez vos parents",
                        "En couple",
                        "Avec d'autres personnes (amis, colocataires, …)"
                    ],
                    "required": true
                },
                {
                    "id": "q4",
                    "text": "Avez-vous un ou plusieurs enfants ?",
                    "type": "boolean",
                    "options": [
                        "Oui",
                        "Non"
                    ],
                    "required": true
                },
                {
                    "id": "q5",
                    "text": "Si oui, êtes-vous seul(e) pour l'élever ou les élever ?",
                    "type": "boolean",
                    "options": [
                        "Oui",
                        "Non"
                    ],
                    "required": false,
                    "condition": {
                        "questionId": "q4",
                        "value": "Oui"
                    }
                },
                {
                    "id": "q6",
                    "text": "Envisagez-vous d'avoir un enfant dans les 5 prochaines années ?",
                    "type": "multiple",
                    "options": [
                        "Oui",
                        "Non",
                        "Je ne sais pas"
                    ],
                    "required": true
                },
                {
                    "id": "q7",
                    "text": "Assumez-vous un rôle d'aidant, c'est-à-dire aidez-vous une personne (adolescente ou adulte) de manière régulière et fréquente, en raison de son âge, de la maladie ou d'une situation de handicap ?",
                    "type": "boolean",
                    "options": [
                        "Oui",
                        "Non"
                    ],
                    "required": true
                },
                {
                    "id": "q8",
                    "text": "En cas de besoin, pouvez-vous compter sur une personne de votre entourage ?",
                    "type": "boolean",
                    "options": [
                        "Oui",
                        "Non"
                    ],
                    "required": true
                },
                {
                    "id": "q9",
                    "text": "Jugez-vous vos conditions de vie, de travail ou d'étude difficiles/pénibles ?",
                    "type": "boolean",
                    "options": [
                        "Oui",
                        "Non"
                    ],
                    "required": true
                }
            ]
        },
        {
            "id": "antecedents",
            "title": "Vos Antécédents Personnels et Familiaux",
            "questions": [
                {
                    "id": "q10",
                    "text": "Avez-vous ou avez-vous eu une de ces maladies ?",
                    "type": "multiple",
                    "options": [
                        "Diabète",
                        "Maladie congénitale",
                        "Allergie(s)",
                        "Cancers",
                        "Maladies respiratoires (asthme…)",
                        "Dépression et/ou troubles anxieux et/ou troubles du comportement alimentaire (anorexie, boulimie…)",
                        "Autre(s) maladie(s)",
                        "Aucune"
                    ],
                    "required": true
                },
                {
                    "id": "q11",
                    "text": "Avez-vous déjà été hospitalisé(e) ?",
                    "type": "boolean",
                    "options": [
                        "Oui",
                        "Non"
                    ],
                    "required": true
                },
                {
                    "id": "q12",
                    "text": "À quand remonte votre dernière prise de sang et/ou une analyse d'urine ?",
                    "type": "multiple",
                    "options": [
                        "Moins de 6 mois",
                        "Plus de 6 mois",
                        "Jamais réalisée"
                    ],
                    "required": true,
                    "helpText": "Merci d'en rapporter les résultats lors de votre Bilan Prévention"
                },
                {
                    "id": "q13",
                    "text": "Des personnes de votre famille proche (mère, père, sœur ou frère) sont ou ont été atteintes de maladies connues ? (diabète, hypertension artérielle, cancer, infarctus, mort subite, hypercholestérolémie familiale...)",
                    "type": "multiple",
                    "options": [
                        "Oui",
                        "Non",
                        "Je ne sais pas"
                    ],
                    "required": true
                }
            ]
        },
        {
            "id": "sante_actuelle",
            "title": "Votre État de Santé Actuel",
            "questions": [
                {
                    "id": "q14",
                    "text": "Suivez-vous actuellement un traitement ?",
                    "type": "boolean",
                    "options": [
                        "Oui",
                        "Non"
                    ],
                    "required": true,
                    "helpText": "Si oui, veuillez amener vos ordonnances lors du Bilan Prévention"
