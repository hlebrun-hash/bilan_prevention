                {
                    "id": "q32",
                    "text": "Avez-vous déjà réalisé un dépistage des infections sexuellement transmissibles ?",
                    "type": "multiple",
                    "options": [
                        "Oui",
                        "Non",
                        "Je ne sais pas"
                    ],
                    "required": true
                },
                {
                    "id": "q33",
                    "text": "Êtes-vous dans l'une des situations suivantes ?",
                    "type": "multiple",
                    "options": [
