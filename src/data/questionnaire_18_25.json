                    "options": [
                        "Plusieurs fois par jour",
                        "Plusieurs fois par semaine",
                        "Une fois par semaine",
                        "Une fois par mois",
                        "Jamais"
                    ],
                    "required": true
                },
                {
                    "id": "q24",
                    "text": "Combien de temps par jour passez-vous assis(e) ou allongé(e) hors période de sommeil ?",
                    "type": "multiple",
                    "options": [
                        "Moins de 2 heures",
                        "Entre 2 et 4 heures",
                        "Entre 4 et 7 heures",
                        "Plus de 7 heures",
                        "Je ne sais pas"
                    ],
                    "required": true
                },
                {
                    "id": "q25",
                    "text": "Combien de temps par jour passez-vous devant un écran en dehors des études ou du travail (téléphone portable, télévision, jeux vidéo, ordinateur) ?",
                    "type": "multiple",
                    "options": [
                        "Moins d'une heure",
                        "Entre 1 heure et 2 heures",
                        "Entre 2 et 4 heures",
                        "Plus de 4 heures",
                        "Je ne sais pas"
                    ],
                    "required": true
                }
            ]
